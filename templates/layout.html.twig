<!DOCTYPE html>
<html>
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>{% block title %}Sortir.com - Login{% endblock %}</title>
    <link rel="stylesheet" href="{{ asset('assets/css/bootstrap.min.css') }}">
    <link href="https://fonts.googleapis.com/css?family=Crete+Round|Maven+Pro|Raleway=swap" rel="stylesheet">
    <link rel="stylesheet" href="{{ asset('assets/css/sortir.css') }}">
    <script src="https://kit.fontawesome.com/96225b8db3.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="{{ asset('assets/css/animate.css') }}">



    {% block stylesheets %}{% endblock %}

</head>
<body id="body_{{ app.request.get('_route') }}">
{{ include ('inc/navbar.html.twig') }}



{% block body %}{% endblock %}


<div class="alerts-container wow fadeInRight">
    {% for label, msgs in app.flashes %}
        {% for msg in msgs %}
            <div class="alert alert-{{ label }}" role="alert">
                {{ msg }}
            </div>
        {% endfor %}
    {% endfor %}
</div>


<script src="{{ asset('assets/js/jquery-3.4.1.min.js') }}"></script>
<script src="{{ asset('assets/js/popper.min.js') }}"></script>
<script src="{{ asset('assets/js/bootstrap.min.js') }}"></script>
<script src="{{ asset('assets/js/wow.min.js') }}"></script>
<script src="{{ asset('assets/js/main.js') }}"></script>
{% block javascripts %}
{% endblock %}
<script>
    $(document).ready(function() {
        new WOW().init();
        $(function () {
            $('[data-toggle="tooltip"]').tooltip()
        })
    });
</script>
</body>
</html>
